{
  "name": "react-prime",
  "version": "4.1.0",
  "description": "A starter kit to create comprehensive React apps with Redux and serverside rendering.",
  "author": "jornbostelaar",
  "repository": {
    "type": "git",
    "url": "https://github.com/JBostelaar/react-redux-boilerplate"
  },
  "license": "MIT",
  "scripts": {
    "start": "node devServer.js",
    "build": "NODE_ENV=production run-p clean build:client build:server",
    "build:client": "webpack -p --hide-modules --config webpack.config.prod.js",
    "build:server": "webpack -p --hide-modules --config webpack.config.server.js",
    "server": "node dist/server.js",
    "clean": "rimraf dist/",
    "lint": "eslint src/",
    "analyzer:build": "NODE_ENV=production webpack -p --config webpack.config.prod.js --profile --json > stats.json",
    "analyzer:run": "webpack-bundle-analyzer stats.json ./dist",
    "analyzer": "run-s bundle-analysis run-analyzer"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel"
  ],
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-css-modules": "^4.7.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "svg-react-loader": "^0.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.9.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "^2.0.2",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-node-externals": "^1.6.0"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0"
    ],
    "plugins": [
      "lodash"
    ]
  },
  "engines": {
    "node": "8.9.1"
  }
}
