{
  "name": "React-Redux-Boilerplate",
  "version": "2.1.0",
  "description": "A starter boilerplate to create comprehensive React apps with Redux and serverside rendering.",
  "author": "jornbostelaar",
  "repository": {
    "type": "git",
    "url": "https://github.com/JBostelaar/react-redux-boilerplate"
  },
  "license": "MIT",
  "scripts": {
    "start": "node devServer.js",
    "build": "NODE_ENV=production run-p clean build:client build:server",
    "build:client": "webpack -p --hide-modules --config webpack.config.prod.js",
    "build:server": "webpack -p --hide-modules --config webpack.config.server.js",
    "server": "node dist/server.js",
    "clean": "rimraf dist/",
    "lint": "eslint src/"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel"
  ],
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-css-modules": "^4.3.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.3",
    "redux": "^3.7.0",
    "redux-thunk": "^2.2.0",
    "svg-react-loader": "^0.4.3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": "8.1.2"
  }
}
